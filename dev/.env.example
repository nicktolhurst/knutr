# ngrok auth (optional but recommended)
NGROK_AUTHTOKEN=
NGROK_DOMAIN=

# Traefik ports (change if 80/443 are blocked)
TRAEFIK_HTTP_PORT=8880
TRAEFIK_HTTPS_PORT=8443

# LLM settings for docker-compose (Ollama default)
LLM_PROVIDER=ollama
OLLAMA_IMAGE=ollama/ollama:latest
OLLAMA_MODEL=llama3

# Prometheus/Tempo/Grafana standard ports
PROM_PORT=9090
GRAFANA_PORT=3000
TEMPO_PORT=3200
